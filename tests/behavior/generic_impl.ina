type Type[T] = {
    inner: T
}

impl[T] Type[T] {
    fn new(inner: T) -> Type[T] {
        Type { inner: inner }
    }

    fn get(self) -> T {
        self.inner
    }

    fn id[U](inner: U) -> Type[U] {
        Type { inner: inner }
    }
}

[test]
fn with_type_params() {
    let t = Type![i32]::new(69);
    assert t.get() == 69;
    let t = Type![str]::id![i64](1);
    assert t.inner == 1;
}
