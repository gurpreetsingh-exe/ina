fn fib(n: i32) -> i32 {
    if n == 0 {
        0
    } else if n == 1 {
        1
    } else {
        fib(n - 1) + fib(n - 2)
    }
}

#[test]
fn test_fib() {
    assert fib(0) == 0;
    assert fib(1) == 1;
    assert fib(2) == 1;
    assert fib(3) == 2;
    assert fib(4) == 3;
    assert fib(10) == 55;
}
