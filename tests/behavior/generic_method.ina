type Test = {}

impl Test {
    fn id[T](self, i: T) -> T {
        Test::id2![T](i)
    }

    fn id2[T](i: T) -> T {
        i
    }
}

#[test]
fn with_type_param() {
    let test = Test {};
    assert test.id![i32](1) == 1;
    assert Test::id![i32](test, 2) == 2;
    assert Test::id2![i64](3) == 3;
}
